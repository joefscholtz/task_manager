 <!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.task_manager.Daemon">
    <method name="GetEventsForMonth">
      <arg name="year" type="i" direction="in"/>
      <arg name="month" type="i" direction="in"/>
      <arg name="events_json" type="s" direction="out"/>
    </method>
    <method name="SyncAllAccounts">
    </method>
    <signal name="SyncCompleted">
      <arg name="success" type="b"/>
    </signal>
    <property name="Version" type="s" access="read"/>
  </interface>
</node>
